<?xml version="1.0"?>
<metalink xmlns="http://www.metalinker.org/" version="3.0">
    <publisher><name>Alaska Satellite Facility</name><url>http://www.asf.alaska.edu/</url></publisher>
    <files>
        {%- for r in results %}
        <file name="{{ r.fileName }}"><resources><url type="http">{{ r.downloadUrl }}</url></resources>{% if r.md5 is defined and r.md5 != 'NA' %}<verification><hash type="md5">{{ r.md5 }}</hash></verification>{% endif %}{% if r.bytes is defined and r.bytes != 'NA' %}<size>{{ r.bytes }}</size>{% endif %}</file>
        {%- endfor %}
    </files>
</metalink>
